<h1>Trial: Hit Enter and See What Happens</h1>

<!-- <p>User is logged in evaluates to: <%= @user_signed_in %></p> -->

<% if !user_signed_in? %>
	<p>You are not logged in.</p>
	<p><%= link_to "Sign Up", new_user_registration_path %></p>
	<p><%= link_to "Log In", new_user_session_path %></p>
<% end %>

<% if user_signed_in? %>
<p>You are logged in as <%= current_user.email %></p>
<p><%= link_to "Log Out", destroy_user_session_path, method: "delete" %></p>
<p></p>
<p><%= link_to "Create a Post", new_post_path %></p>
<% end %>



